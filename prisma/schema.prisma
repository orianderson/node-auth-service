// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = env("DATABASE_URL")
}

model Users {
  id         String   @id
  name       String
  job        String
  email      String
  phone      String
  whatsapp   String
  state      String
  city       String
  picture    String?
  password   String?
  created_at DateTime @default(now())

  @@map("users")
}
